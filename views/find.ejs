<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title>coffee.io | Locations</title>
    <link rel="stylesheet" href="style.css">
</head>

<body>
    <header>
        <div class="container">
            <div id="branding">
                <a href="/index"><img src="img/mug.png" title="Home" alt="mug clicked takes you to home page"></a>

            </div>
            <nav>
                <u1>
                    <li><a href="/signup">Sign Up</a></li>
                    <li><a href="/login">Login</a></li>
                    <li><a href="/about">About</a></li>
                </u1>
            </nav>
        </div>
    </header>
    <div class="wrapper">
        <div class="menu">
<% shops.forEach(shop => { %>
        <div class="single-menu">
            <div class="menu-content">
                <h4><%= shop.name %></h4> 
                <button class="button button1">Select</button>
            </div>
        </div>
<% }) %>
    <footer>
        <p>coffee.io, Copyright &copy; 2020</p>
    </footer>
</body>

</html>